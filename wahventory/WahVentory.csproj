<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Dalamud.NET.Sdk/14.0.0">
  <PropertyGroup>
    <AssemblyName>wahventory</AssemblyName>
    <Version>4.1.0.0</Version>
    <Description>Advanced inventory management</Description>
    <PackageProjectUrl>https://github.com/yourusername/wahventory</PackageProjectUrl>
    <PackageLicenseExpression>AGPL-3.0-or-later</PackageLicenseExpression>
    <IsPackable>false</IsPackable>
    <NoWarn>$(NoWarn);CS8600</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="ECommons" Version="3.1.0.2" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>
</Project>
